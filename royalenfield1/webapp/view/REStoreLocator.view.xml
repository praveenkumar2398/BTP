<mvc:View controllerName="ns.royalenfield.controller.REStoreLocator" xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true" xmlns="sap.m" xmlns:tnt="sap.tnt"
    xmlns:core="sap.ui.core" xmlns:html="http://www.w3.org/1999/xhtml">
    <tnt:ToolPage id="toolPage">
        <tnt:header>
            <tnt:ToolHeader>
                <Title text="Royal Enfield Store" />
            </tnt:ToolHeader>
        </tnt:header>
        <tnt:sideContent>
            <tnt:SideNavigation>
                <tnt:NavigationList>
                    <tnt:NavigationListItem text="India" select="onSelectIndia" />
                </tnt:NavigationList>
                <tnt:fixedItem></tnt:fixedItem>
            </tnt:SideNavigation>
        </tnt:sideContent>
        <tnt:mainContents>
            <NavContainer id="pageContainer" initialPage="pageUSA">
                <pages>
                    <ScrollContainer id="pageIndia" horizontal="false" vertical="true" height="100%">
                        <html:iframe height="1000px" width="100%"
                            src='https://www.royalenfield.com/in/en/locate-us/dealers/'></html:iframe>
                    </ScrollContainer>
                </pages>
            </NavContainer>
        </tnt:mainContents>
    </tnt:ToolPage>
</mvc:View>